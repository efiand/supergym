{% extends '../main.twig' %}

{% block main %}
	{% include '../components/about.twig' with about %}

	{% include '../components/accent-section.twig' with {
		bemMix: 'page__subscriptions',
		component: 'tabs',
		decor: subscriptions.decor,
		data: {
			component: 'cards-list',
			listComponent: 'card',
			heading: subscriptions.heading,
			tabs: subscriptions.groups
		}
	} %}

	{% include '../components/event.twig' with event|merge({
		bemMix: 'page__event container'
	}) %}

	{% include '../components/accent-section.twig' with {
		component: 'rates',
		data: advantages
	} %}
{% endblock %}
